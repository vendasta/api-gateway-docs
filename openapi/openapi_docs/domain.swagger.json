        "modifiers": {
          "type": "array",
          "items": {
            "type": "object",
            "$ref": "#/definitions/v1Modifier"
          },
          "title": "Expansion of the spec"
        },
        "raw": {
          "type": "string",
          "title": "Raw SPF record"
        }
      },
      "title": "SPF (Sender Policy Framework) Records declare which servers can send email on behalf of\na particular domain"
    },
