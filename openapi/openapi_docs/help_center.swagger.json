    "/helpcenter.v1.HelpCenter/IndexArticle": {
      "post": {
        "summary": "Index an article from into vstore to be searched",
        "operationId": "HelpCenter_IndexArticle",
        "responses": {
          "200": {
            "description": "A successful response.",
            "schema": {
              "type": "object",
              "properties": {}
            }
          },
          "default": {
            "description": "An unexpected error response.",
            "schema": {
              "$ref": "#/definitions/rpcStatus"
